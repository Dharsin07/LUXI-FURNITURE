{
  "name": "luxi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "cd react-app && npm run build",
    "preview": "vite preview",
    "dev:react-app": "cd react-app && npm run dev",
    "start:react-app": "cd react-app && npm start",
    "start:backend": "cd backend && npm start",
    "start": "cd backend && npm start",
    "start:full": "concurrently \"npm run start:backend\" \"npm run start:react-app\"",
    "dev:full": "concurrently \"npm run start:backend\" \"npm run dev:react-app\"",
    "install:all": "npm install && cd react-app && npm install && cd ../backend && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "gh-pages": "^6.3.0",
    "vite": "^7.1.9"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
